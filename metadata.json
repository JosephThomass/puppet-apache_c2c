{
  "name": "camptocamp-apache_c2c",
  "version": "1.0.0",
  "author": "Camptocamp",
  "summary": "Puppet Apache module",
  "license": "Apache-2.0",
  "source": "https://github.com/camptocamp/puppet-apache_c2c",
  "project_page": "https://github.com/camptocamp/puppet-apache_c2c",
  "issues_url": "https://github.com/camptocamp/puppet-apache_c2c/issues",
  "dependencies": [
    {
      "name": "camptocamp/collectd"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=1.0.0 <2.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.2.0 <5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
